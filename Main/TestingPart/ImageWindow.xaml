<Window x:Class="Main.TestingPart.ImageWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Main.TestingPart"
        mc:Ignorable="d"
        Title="ImageWindow" Height="304" Width="410">
    <Grid>
        <Viewbox Stretch="Fill" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Image SourceUpdated="Graph_SourceUpdated" x:FieldModifier="public" x:Name="Graph" RenderOptions.BitmapScalingMode="HighQuality"/>
        </Viewbox>
    </Grid>

</Window>
